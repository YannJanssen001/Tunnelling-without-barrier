Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"ClearAll", "[", "generatePlotAndRoots", "]"}], ";"}], 
  "\[IndentingNewLine]", "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{
   "Function", " ", "to", " ", "generate", " ", "plots", " ", "and", " ", 
    "roots", " ", "for", " ", "given", " ", "px", " ", "and", " ", "py"}], 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"generatePlotAndRoots", "[", 
    RowBox[{"px_", ",", "py_", ",", "seeds_", ",", "k_"}], "]"}], ":=", 
   "\[IndentingNewLine]", 
   RowBox[{"Module", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "allSolutions", ",", "complexSolutions", ",", "nonrepeatedSolutions", 
       ",", "tValues", ",", "tolerance", ",", "filteredTValues"}], "}"}], ",",
      "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{"Find", " ", "roots"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"allSolutions", "=", 
       RowBox[{"Round", "[", 
        RowBox[{
         RowBox[{"t", "/.", " ", 
          RowBox[{"Quiet", "[", 
           RowBox[{"Table", "[", 
            RowBox[{
             RowBox[{"FindRoot", "[", 
              RowBox[{
               RowBox[{
                RowBox[{"DsFunc", "[", 
                 RowBox[{"t", ",", 
                  RowBox[{"{", 
                   RowBox[{"px", ",", "py"}], "}"}], ",", "k"}], "]"}], "==", 
                "0"}], ",", 
               RowBox[{"{", 
                RowBox[{"t", ",", "tseed"}], "}"}]}], "]"}], ",", 
             RowBox[{"{", 
              RowBox[{"tseed", ",", "seeds"}], "}"}]}], "]"}], "]"}]}], ",", 
         "0.0001"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"complexSolutions", "=", 
       RowBox[{"Select", "[", 
        RowBox[{"allSolutions", ",", 
         RowBox[{
          RowBox[{
           RowBox[{"Im", "[", "#", "]"}], ">", "0"}], "&"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"nonrepeatedSolutions", "=", 
       RowBox[{"DeleteDuplicates", "[", "complexSolutions", "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"tValues", "=", 
       RowBox[{"SortBy", "[", 
        RowBox[{
         RowBox[{"Select", "[", 
          RowBox[{"nonrepeatedSolutions", ",", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{
              RowBox[{
               RowBox[{"Re", "[", "#", "]"}], ">=", "realMin"}], "&&", 
              RowBox[{
               RowBox[{"Re", "[", "#", "]"}], "<=", "realMax"}]}], ")"}], 
            "&"}]}], "]"}], ",", 
         RowBox[{
          RowBox[{"Re", "[", "#", "]"}], "&"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{
       "Define", " ", "a", " ", "tolerance", " ", "for", " ", "considering", 
        " ", "values", " ", "as", " ", "0"}], "*)"}], "\[IndentingNewLine]", 
      RowBox[{"tolerance", "=", 
       RowBox[{"10", "^", 
        RowBox[{"-", "3"}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"filteredTValues", "=", 
       RowBox[{"Select", "[", 
        RowBox[{"tValues", ",", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"Abs", "[", 
             RowBox[{"Re", "[", 
              RowBox[{"DsFunc", "[", 
               RowBox[{"#", ",", 
                RowBox[{"{", 
                 RowBox[{"px", ",", "py"}], "}"}], ",", "k"}], "]"}], "]"}], 
             "]"}], "<=", "tolerance"}], "&&", 
           RowBox[{
            RowBox[{"Abs", "[", 
             RowBox[{"Im", "[", 
              RowBox[{"DsFunc", "[", 
               RowBox[{"#", ",", 
                RowBox[{"{", 
                 RowBox[{"px", ",", "py"}], "}"}], ",", "k"}], "]"}], "]"}], 
             "]"}], "<=", "tolerance"}]}], "&"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"{", "filteredTValues", "}"}]}]}], "]"}]}], ";"}]}], "Input",
