   "\n", 
   RowBox[{"(*", 
    RowBox[{
    "Define", " ", "the", " ", "vector", " ", "potential", " ", 
     "components"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"Ax", "[", "tx_", "]"}], ":=", 
     RowBox[{
      RowBox[{"-", 
       RowBox[{"Integrate", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"E1", "[", "t", "]"}], "[", 
          RowBox[{"[", "1", "]"}], "]"}], ",", "t"}], "]"}]}], "/.", " ", 
      RowBox[{"t", "->", "tx"}]}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{
     RowBox[{"Ay", "[", "tx_", "]"}], ":=", 
     RowBox[{
      RowBox[{"-", 
       RowBox[{"Integrate", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"E1", "[", "t", "]"}], "[", 
          RowBox[{"[", "2", "]"}], "]"}], ",", "t"}], "]"}]}], "/.", " ", 
      RowBox[{"t", "->", "tx"}]}]}], ";"}], "\n", 
   RowBox[{"(*", 
    RowBox[{
