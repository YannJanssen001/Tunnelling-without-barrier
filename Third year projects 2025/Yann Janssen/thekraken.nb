      "A"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"A", "[", "tx_", "]"}], ":=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"Ax", "[", "tx", "]"}], ",", 
       RowBox[{"Ay", "[", "tx", "]"}]}], "}"}]}], ";"}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{"Define", " ", "Sf"}], ",", "Sfunc", ",", "DsFunc", ",", 
