Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
   "Function", " ", "to", " ", "generate", " ", "plots", " ", "and", " ", 
    "roots", " ", "for", " ", "given", " ", "px", " ", "and", " ", "py"}], 
   "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"generatePlotAndRoots", "[", 
      RowBox[{"px_", ",", "py_"}], "]"}], ":=", "\[IndentingNewLine]", 
     RowBox[{"Module", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
        "realMin", ",", "realMax", ",", "imagMin", ",", "imagMax", ",", 
         "realSteps", ",", "imagSteps", ",", "listofseeds", ",", 
         "allSolutions", ",", "complexSolutions", ",", "nonrepeatedSolutions",
          ",", "tValues", ",", "tolerance", ",", "filteredTValues"}], "}"}], 
       ",", "\[IndentingNewLine]", "\[IndentingNewLine]", 
       RowBox[{"(*", 
        RowBox[{
        "Generate", " ", "the", " ", "grid", " ", "of", " ", "seeds"}], 
        "*)"}], "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"realMin", "=", 
         RowBox[{"-", "2.133"}]}], ";", 
        RowBox[{"realMax", "=", "4.15"}], ";", "\[IndentingNewLine]", 
        RowBox[{"imagMin", "=", "0"}], ";", 
        RowBox[{"imagMax", "=", "5"}], ";", "\[IndentingNewLine]", 
        RowBox[{"realSteps", "=", "10"}], ";", 
        RowBox[{"imagSteps", "=", "10"}], ";", "\[IndentingNewLine]", 
        "\[IndentingNewLine]", 
        RowBox[{"listofseeds", "=", 
         RowBox[{"Flatten", "[", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"Table", "[", "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{"x", "+", 
              RowBox[{"I", "  ", "y"}]}], ",", "\[IndentingNewLine]", 
             RowBox[{"{", 
              RowBox[{"x", ",", "realMin", ",", "realMax", ",", 
               RowBox[{
                RowBox[{"(", 
                 RowBox[{"realMax", "-", "realMin"}], ")"}], "/", 
                "realSteps"}]}], "}"}], ",", 
             RowBox[{"{", 
              RowBox[{"y", ",", "imagMin", ",", "imagMax", ",", 
               RowBox[{
                RowBox[{"(", 
                 RowBox[{"imagMax", "-", "imagMin"}], ")"}], "/", 
                "imagSteps"}]}], "}"}]}], "]"}], ",", "1"}], "]"}]}], ";", 
        "\[IndentingNewLine]", "\[IndentingNewLine]", 
        RowBox[{"(*", 
         RowBox[{"Find", " ", "roots"}], "*)"}], "\[IndentingNewLine]", 
        RowBox[{"allSolutions", "=", 
         RowBox[{"Round", "[", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"t", "/.", " ", 
            RowBox[{"Quiet", "[", "\[IndentingNewLine]", 
             RowBox[{"Table", "[", 
              RowBox[{
               RowBox[{"FindRoot", "[", "\[IndentingNewLine]", 
                RowBox[{
                 RowBox[{
                  RowBox[{"DsFunc", "[", 
                   RowBox[{"t", ",", 
                    RowBox[{"{", 
                    RowBox[{"px", ",", "py"}], "}"}]}], "]"}], "==", "0"}], 
                 ",", "\[IndentingNewLine]", 
                 RowBox[{"{", 
                  RowBox[{"t", ",", "tseed"}], "}"}]}], "]"}], ",", 
               RowBox[{"{", 
                RowBox[{"tseed", ",", "listofseeds"}], "}"}]}], "]"}], 
             "]"}]}], ",", "0.0001"}], "]"}]}], ";", "\[IndentingNewLine]", 
        "\[IndentingNewLine]", 
        RowBox[{"complexSolutions", "=", 
         RowBox[{"Select", "[", 
          RowBox[{"allSolutions", ",", 
           RowBox[{
            RowBox[{
             RowBox[{"Im", "[", "#", "]"}], ">", "0"}], "&"}]}], "]"}]}], ";",
         "\[IndentingNewLine]", "\[IndentingNewLine]", 
        RowBox[{"nonrepeatedSolutions", "=", 
         RowBox[{"DeleteDuplicates", "[", "complexSolutions", "]"}]}], ";", 
        "\[IndentingNewLine]", "\[IndentingNewLine]", 
        RowBox[{"tValues", "=", 
         RowBox[{"SortBy", "[", 
          RowBox[{
           RowBox[{"Select", "[", 
            RowBox[{"nonrepeatedSolutions", ",", 
             RowBox[{
              RowBox[{"(", 
               RowBox[{
                RowBox[{
                 RowBox[{"Re", "[", "#", "]"}], ">=", "realMin"}], "&&", 
                RowBox[{
                 RowBox[{"Re", "[", "#", "]"}], "<=", "realMax"}]}], ")"}], 
              "&"}]}], "]"}], ",", 
           RowBox[{
            RowBox[{"Re", "[", "#", "]"}], "&"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", "\[IndentingNewLine]", 
        RowBox[{"(*", 
         RowBox[{
         "Define", " ", "a", " ", "tolerance", " ", "for", " ", "considering",
           " ", "values", " ", "as", " ", "0"}], "*)"}], 
        "\[IndentingNewLine]", 
        RowBox[{"tolerance", "=", 
         RowBox[{"10", "^", 
          RowBox[{"-", "3"}]}]}], ";", "\[IndentingNewLine]", 
        "\[IndentingNewLine]", 
        RowBox[{"filteredTValues", "=", 
         RowBox[{"Select", "[", 
          RowBox[{"tValues", ",", 
           RowBox[{
            RowBox[{
             RowBox[{
              RowBox[{"Abs", "[", 
               RowBox[{"Re", "[", 
                RowBox[{"DsFunc", "[", 
                 RowBox[{"#", ",", 
                  RowBox[{"{", 
                   RowBox[{"px", ",", "py"}], "}"}]}], "]"}], "]"}], "]"}], "<=",
               "tolerance"}], "&&", 
             RowBox[{
